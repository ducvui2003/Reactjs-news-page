{
    "version": 2,
    "buildCommand": "npm run build",
    "git": {
        "deploymentEnabled": {
            "main": false
        }
    },
    "devCommand": "npm run dev",
    "framework": "vite",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}